{
  "name": "belle-bdp",
  "main": "main.js",
  "version": "0.2.6",
  "author": "guo.mk",
  "description": "electron测试公司",
  "scripts": {
    "dev": "nodemon  app.js",
    "start": "electron .",
    "build": "build --win",
    "ship": "build --win -p always"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.45.0"
  },
  "dependencies": {
    "asar": "^0.14.0",
    "electron-log": "^2.2.11",
    "electron-updater": "^2.16.1",
    "koa": "^2.4.1",
    "koa-static": "^4.0.1",
    "nodemon": "^1.12.1",
    "semver": "^5.4.1"
  },
  "build": {
    "appId": "com.github.gmkgmk.belleddp",
    "productName": "测试名称",
    "publish": {
      "provider": "generic",
      "url": "http://www.gmkgmk.com:8080"
    },
    "nsis": {
      "artifactName": "belle-bdp-${version}.${ext}",
      "runAfterFinish": false,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "installerLanguages": "zh_CN",
      "language": 2052,
      "perMachine":false
    },
    "win": {
      "target": "nsis",
      "icon": "icon/icon.ico"
    }
  },
  "repository": "https://github.com/gmkgmk/electron-up"
}
